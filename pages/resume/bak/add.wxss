.container {
    background-color: #F2f2f2;
    flex-direction: column;
    flex: column;
}

form{
  width: 100%;
}

.cell {
	display: flex;
	flex-direction: row;
	justify-content: flex-start;
	align-items:center;
	margin: 4px 6px 6px 4px;
	border-bottom: 1px solid #eee;
	line-height: 28px;
}

.cell:last-child {
	border-bottom: 0;
}

/*label显示字符*/
.cell text:first-child {
	width: 50px;
}
	
.div{
  display: flex;
	flex-direction: column;
}  

.form-com {
  font-size: 12px;
	width: 100%;
  margin-bottom: 100px;
}

.form-submit{
  position: fixed;
	bottom: 0;
	width: 100%;
}